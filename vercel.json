{
    "version": 2,
    "builds": [
     {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
        {
            "src": "/socket.io/*",
            "dest": "server.js"
          },
     {
      "src": "/(.*)",
      "dest": "server.js"
     }
    ],
    "websocket": {
    "enabled": true
  }
}
   
