{
    "version": 2,
    "builds": [
     {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
        {
            "src": "/socket.io/*",
            "dest": "server.js",
            "headers": {
        "Upgrade": "websocket"
      }
          },
     {
      "src": "/(.*)",
      "dest": "server.js"
     }
    ]
}
   
